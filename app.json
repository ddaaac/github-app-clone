{
  "expo": {
    "name": "github-app-clone",
    "slug": "github-app-clone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/github-icon.png",
    "splash": {
      "image": "./assets/github-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "useNextNotificationsApi": true
    }
  }
}
